{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "ng build",
          "outputDirectory": "dist/my-app"
        }
      }
    ],
    "routes": [
      {
        "src": "/(assets/.+)",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }